                        <div class="main-header-content">
                            <!-- logo start -->
                            <a href="#" class="logo">
                                <div class="img-wrap">
                                    <img src="" alt="logo">
                                </div>
                                <div class="title-block">
                                    <h6 class="logo-title">@Model.logo</h6>
                                    <div class="sub-title">SOCIAL NETWORK</div>
                                </div>
